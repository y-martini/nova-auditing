{
    "name": "y-martini/nova-auditing",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "~4",
        "owen-it/laravel-auditing": "^13.1"
    },
    "autoload": {
        "psr-4": {
            "YMartini\\Nova\\Auditing\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YMartini\\Nova\\Auditing\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
